{
  "presets": [[
    "module:metro-react-native-babel-preset",
    {
      "useTransformReactJSXExperimental": "true"
    }
  ]],
  "plugins": [
    "inline-dotenv",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@greeneggs": "./src"
        }
      }
    ],
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic"
      }
    ]
  ]
}
